{
	"kit": {
		"alias": {
			"$actions/*": "./src/actions/*",
			"$components/*": "./src/components/*",
			"$data/*": "./src/data/*",
			"$routes/*": "./src/routes/*",
			"$runes/*": "./src/runes/*",
			"$styles/*": "./src/styles/*",
			"$svg/*": "./src/svg/*",
			"$utils/*": "./src/utils/*",
		}
	},
	"include": [
		"src/**/*.js",
		"src/**/*.svelte"
	],
	"exclude": [
		"tasks/*.js"
	],
	"extends": "./.svelte-kit/tsconfig.json"
}